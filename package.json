{
  "name": "ag-quota",
  "displayName": "Antigravity Quota (AGQ)",
  "description": "%ext.description%",
  "version": "1.2.0",
  "publisher": "henrikdev",
  "icon": "assets/AntiGravityQuota.png",
  "l10n": "./l10n",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:agq.refresh",
    "onCommand:agq.show_menu",
    "onCommand:agq.activate",
    "onCommand:agq.reconnect",
    "onCommand:agq.show_logs"
  ],
  "main": "./export/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "agq.refresh",
        "title": "%commands.refresh.title%"
      },
      {
        "command": "agq.reconnect",
        "title": "%commands.reconnect.title%"
      },
      {
        "command": "agq.show_logs",
        "title": "%commands.showLogs.title%"
      }
    ],
    "configuration": {
      "title": "AGQ",
      "properties": {
        "agq.enabled": {
          "type": "boolean",
          "default": true,
          "description": "%config.enabled.description%",
          "order": 1
        },
        "agq.pollingInterval": {
          "type": "number",
          "default": 120,
          "minimum": 30,
          "description": "%config.pollingInterval.description%",
          "order": 2
        },
        "agq.pinnedModels": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          },
          "description": "%config.pinnedModels.description%",
          "order": 3
        },
        "agq.showPromptCredits": {
          "type": "boolean",
          "default": false,
          "description": "%config.showPromptCredits.description%",
          "order": 4
        },
        "agq.useSimpleNames": {
          "type": "boolean",
          "default": true,
          "description": "%config.useSimpleNames.description%",
          "order": 5
        }
      }
    }
  },
  "scripts": {
    "node:vsix:package": "npm run compile && npx vsce package",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vscode/vsce": "3.7.1",
    "eslint": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/morwapi/AntigravityQuotaPlus.git"
  },
  "keywords": [
    "antigravity",
    "quota",
    "ai"
  ],
  "author": "Henrik Mertens",
  "license": "MIT"
}